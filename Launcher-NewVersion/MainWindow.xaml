                    <Button Name="News3" IsEnabled="False" Style="{StaticResource LeftMouseButton}" Background="{x:Null}" Foreground="#FF918078" BorderBrush="{x:Null}" FontSize="13" Height="18" VerticalAlignment="Top" FontFamily="Myriad Pro" Margin="0,5,0,0" VerticalContentAlignment="Center"/>
                    <Button Name="News4" IsEnabled="False" Style="{StaticResource LeftMouseButton}" Background="{x:Null}" Foreground="#FF918078" BorderBrush="{x:Null}" FontSize="13" Height="18" VerticalAlignment="Top" FontFamily="Myriad Pro" Margin="0,5,0,0" VerticalContentAlignment="Center"/>
                    <Button Name="News5" IsEnabled="False" Style="{StaticResource LeftMouseButton}" Background="{x:Null}" Foreground="#FF918078" BorderBrush="{x:Null}" FontSize="13" Height="20" VerticalAlignment="Top" FontFamily="Myriad Pro" Margin="0,5,0,0" VerticalContentAlignment="Center"/>
                </StackPanel>
                <StackPanel HorizontalAlignment="Stretch"  Panel.ZIndex="10" Margin="5,0,0,0" Grid.Row="1" Grid.Column="1">
                    <Button Name="lbl1" IsEnabled="False" Style="{StaticResource LeftMouseButton}" Background="{x:Null}" Foreground="#FF918078" BorderBrush="{x:Null}" FontSize="13" Height="18" VerticalAlignment="Top" FontFamily="Myriad Pro" Content="" VerticalContentAlignment="Center" HorizontalAlignment="Left"/>
                    <Button Name="lbl2" IsEnabled="False" Style="{StaticResource LeftMouseButton}" Background="{x:Null}" Foreground="#FF918078" BorderBrush="{x:Null}" FontSize="13" Height="18" VerticalAlignment="Top" FontFamily="Myriad Pro" Content="" Margin="0,5,0,0" VerticalContentAlignment="Center" HorizontalAlignment="Left"/>
                    <Button Name="lbl3" IsEnabled="False" Style="{StaticResource LeftMouseButton}" Background="{x:Null}" Foreground="#FF918078" BorderBrush="{x:Null}" FontSize="13" Height="18" VerticalAlignment="Top" FontFamily="Myriad Pro" Content="" Margin="0,5,0,0" VerticalContentAlignment="Center" HorizontalAlignment="Left"/>
                    <Button Name="lbl4" IsEnabled="False" Style="{StaticResource LeftMouseButton}" Background="{x:Null}" Foreground="#FF918078" BorderBrush="{x:Null}" FontSize="13" Height="18" VerticalAlignment="Top" FontFamily="Myriad Pro" Content="" Margin="0,5,0,0" VerticalContentAlignment="Center" HorizontalAlignment="Left"/>
                    <Button Name="lbl5" IsEnabled="False" Style="{StaticResource LeftMouseButton}" Background="{x:Null}" Foreground="#FF918078" BorderBrush="{x:Null}" FontSize="13" Height="20" VerticalAlignment="Top" FontFamily="Myriad Pro" Content="" Margin="0,5,0,0" VerticalContentAlignment="Center" HorizontalAlignment="Left"/>
                </StackPanel>
            </Grid>
            
